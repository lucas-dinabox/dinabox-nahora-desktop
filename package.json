{
  "name": "nahora",
  "version": "1.0.0",
  "description": "Controle de ponto",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "nahora 1.0.0",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "my-app-uninstaller",
      "license": "license.md",
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^22.0.2",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "axios": "^1.2.6",
    "electron-browser-storage": "^1.0.7",
    "electron-store": "^8.1.0"
  }
}
